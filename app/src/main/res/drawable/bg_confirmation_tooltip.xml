<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#8000" />
            <stroke
                android:width="1sp"
                android:color="#FFF" />
            <corners android:radius="10sp" />
        </shape>
    </item>
    <item
        android:bottom="2sp"
        android:left="2sp"
        android:right="2sp"
        android:top="2sp">
        <shape android:shape="rectangle">
            <solid android:color="#8000" />
            <stroke
                android:width="1sp"
                android:color="#FFF" />
            <corners android:radius="4sp" />
        </shape>
    </item>
</layer-list>
